    "version": "1.0.0+1",
    "configurations": [
      {
        "name": "debug",
        "request": "launch",
        "type": "dart"
      },
      {
        "name": "Web debug",
        "request": "launch",
        "type": "dart",
        "deviceId": "chrome",
        "args": ["--web-port=8888", "--web-renderer canvaskit"]
      },
      {
        "name": "Web release",
        "request": "launch",
        "type": "dart",
        "deviceId": "chrome",
        "flutterMode": "release",
  
        "args": ["--web-port=8888", "--web-renderer canvaskit"]
      },
      {
        "name": "Web release Html",
        "request": "launch",
        "type": "dart",
        "deviceId": "chrome",
        "flutterMode": "release",
  
        "args": ["--web-port=8888", "--web-renderer html"]
      }
    ]
  }
  